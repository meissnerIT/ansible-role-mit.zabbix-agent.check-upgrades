LIBS=$(checkrestart|awk '! /COMMAND/ { print $4; }'|sort -u| tr "\n" ","|sed 's/,$//')

zabbix_sender -c /usr/local/etc/zabbix64/zabbix_agentd.conf -k "system.sw.needrestart.library" -o "$LIBS" 1>/dev/null

