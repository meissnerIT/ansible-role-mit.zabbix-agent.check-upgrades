# 2024-02-09: Beautified output

LIBS=$(sudo /usr/local/sbin/checkrestart|awk '! /COMMAND/ { print $4; }'|sort -u| tr "\n" ","|sed 's/,$//')

zabbix_sender -c /etc/zabbix/zabbix_agentd.conf -k "system.sw.needrestart.library" -o "$LIBS" 1>/dev/null

